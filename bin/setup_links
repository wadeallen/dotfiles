#! /bin/bash

# Run this script on new Ubuntu installs once Documents has been installed and synced

cd /home/wadeallen/ &
rm -Rf Documents &
rm -Rf Music &
rm -Rf Pictures &
rm -Rf Public &
rm -Rf Templates &
rm -Rf Videos &
ln -s Documents/Editing/ Editing &
ln -s Documents/Music/ Music &
ln -s Documents/Pictures/ Pictures &
ln -s Documents/Print/ Print &
ln -s Documents/Scans/ Scans &
ln -s Documents/Sites/ Sites &
ln -s Documents/Videos/ Videos &
ln -s Documents/bibles bibles

