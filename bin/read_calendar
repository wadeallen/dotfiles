#!/usr/bin/env python3

from icalevents.icalevents import events
import datetime
from dateutil import relativedelta
from pprint import pprint

today = datetime.date.today()
two_weeks = datetime.date.today() + relativedelta.relativedelta(days=14)

es  = events('webcal://calendar.planningcenteronline.com/icals/eJxj4ajmsGLLz2TSZLLiSi3OLygB8Ys9lfgTc3LiU8tS80qK2azYXEOs2EozmbbPs2IvK_FUEiwoTcrJTI4vycxNLWaz5gix4i5ILErMLa5mAACTfRf979595f57134bfba4df1d68575e5f050d0a2c75e1', fix_apple=True, start=today, end=two_weeks)

# for e in es:
# 	pprint(vars(e))

events=[]

for e in es:
	events.append(e.start.strftime("%Y-%m-%d %I:%M %p"))

print(events)

